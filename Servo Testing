#include <kipr/botball.h>

int main()
{
enable_servo(3);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
msleep(200);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
msleep(200);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
msleep(200);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
msleep(200);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
msleep(200);
set_servo_position(3,1024);
msleep(200);
set_servo_position(3,0);
msleep(200);
set_servo_position(3,948);
msleep(100);
disable_servo(3);
}

