#include <kipr/botball.h>

int main()
{
create_connect();
printf("Left cliff sensor reads %d\n", get_create_lcliff_amt());
printf("Right cliff sensor reads %d\n", get_create_rcliff_amt());
while ((get_create_lcliff_amt()>1500) && (get_create_rcliff_amt()>1500))
{
	while (get_create_lfcliff_amt()<1500)
	{
	create_drive_direct(200,50);
	msleep(10);
	}
	while (get_create_rfcliff_amt()>1500)
	{
	create_drive_direct(50,200);
	msleep(10);
    {
}
create_stop();
create_disconnect();
return 0;
}
}
}
